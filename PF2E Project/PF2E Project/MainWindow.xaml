<Window x:Class="PF2E_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PF2E_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden"/>
        <Button Content="Create Character" Click="CreateCharacter"/>
    </Grid>
    <Page x:Class="PF2E_Project.CharacterCreation"
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:local="clr-namespace:PF2E_Project"
          mc:Ignorable="d"
          Title="CharacterCreation" Height="450" Width="800">
        <Grid>
            <Button Content="Set Name" Click="SetName"/>
            
        </Grid>
    </Page>
</Window>
